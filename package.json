{
  "name": "@puffmeow/schema2ts",
  "version": "0.1.2",
  "description": "Transform JSON Schema to Typescript interface automatically",
  "files": [
    "dist"
  ],
  "main": "./dist/src/index.umd.cjs",
  "module": "./dist/src/index.js",
  "exports": {
    ".": {
      "import": "./dist/src/index.js",
      "require": "./dist/src/index.umd.cjs"
    }
  },
  "types": "./dist/src/index.d.ts",
  "type": "module",
  "repository": {
    "url": "https://github.com/PuffMeow/schema2ts"
  },
  "scripts": {
    "build": "vite build",
    "test:watch": "vitest",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "pub": "npm run build && npm run test && npm publish"
  },
  "keywords": [
    "schema to typescript",
    "transformer",
    "schema2ts"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/node": "^18.14.1",
    "@vitest/coverage-c8": "^0.33.0",
    "prettier": "2.8.4",
    "tslib": "^2.5.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.7",
    "vitest": "^0.33.0"
  },
  "dependencies": {
    "@puffmeow/rusty-schema2ts": "^0.0.1"
  }
}
