  "devDependencies": {
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/node": "^18.14.1",
    "@vitest/coverage-c8": "^0.29.2",
    "prettier": "2.8.4",
    "tslib": "^2.5.0",
    "typescript": "^5.0.2",
    "vite": "^4.1.4",
    "vitest": "^0.29.2"
  }
