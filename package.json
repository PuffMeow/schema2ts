{
  "name": "@puffmeow/schema2ts",
  "version": "0.2.2",
  "description": "Transform JSON Schema to Typescript interface automatically",
  "files": [
    "dist"
  ],
  "main": "./dist/src/index.umd.cjs",
  "module": "./dist/src/index.js",
  "exports": {
    ".": {
      "import": "./dist/src/index.js",
      "require": "./dist/src/index.umd.cjs"
    }
  },
  "types": "./dist/src/index.d.ts",
  "type": "module",
  "repository": {
    "url": "https://github.com/PuffMeow/schema2ts"
  },
  "scripts": {
    "build": "vite build",
    "test:watch": "vitest",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "pub": "npm run build && npm run test && npm publish"
  },
  "keywords": [
    "jsonschema to interface",
    "jsonschema to ts",
    "schema to typescript",
    "schema to ts",
    "schema to interface",
    "schema to type"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.2",
    "@types/node": "^20.4.9",
    "@vitest/coverage-v8": "^0.34.1",
    "prettier": "^2.8.8",
    "tslib": "^2.6.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.9",
    "vitest": "^0.34.1"
  }
}
